{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "5.5.0",
        "@kbase/ui-components": "0.2.1-alpha.0",
        "@kbase/ui-lib": "0.2.0-alpha.3",
        "@types/jest": "24.0.18",
        "@types/marked": "0.6.5",
        "@types/node": "12.7.9",
        "@types/react": "16.9.4",
        "@types/react-dom": "16.9.1",
        "@types/react-redux": "7.1.4",
        "antd": "3.23.5",
        "craco-antd": "1.12.0",
        "http-proxy-middleware": "0.20.0",
        "marked": "0.7.0",
        "morgan": "1.9.1",
        "nodemon": "1.19.3",
        "react": "16.10.1",
        "react-dom": "16.10.1",
        "react-redux": "7.1.1",
        "react-scripts": "3.2.0",
        "redux": "4.0.4",
        "redux-thunk": "2.3.0",
        "typescript": "3.6.3"
    },
    "resolutions": {
        "**/less-loader": "5.0.0"
    },
    "scripts": {
        "start": "nodemon -w ./craco.config.js -w ./src/custom/style/antd/theme.less --exec 'craco start'",
        "build": "craco build",
        "test": "react-scripts test",
        "install-plugin": "bash scripts/install-plugin.bash"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "xproxy": "https://ci.kbase.us",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}